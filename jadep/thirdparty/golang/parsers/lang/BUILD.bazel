load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "lang_proto",
    srcs = ["lang.proto"],
    visibility = ["//visibility:public"],
)

go_proto_library(
    name = "lang_go_proto",
    importpath = "github.com/bazelbuild/tools_jvm_autodeps/jadep/thirdparty/golang/parsers/lang",
    proto = ":lang_proto",
    visibility = ["//visibility:public"],
)

go_library(
    name = "go_default_library",
    embed = [":lang_go_proto"],
    importpath = "github.com/bazelbuild/tools_jvm_autodeps/jadep/thirdparty/golang/parsers/lang",
    visibility = ["//visibility:public"],
)
